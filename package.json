{
  "name": "shuffle-airdrop-scam-analysis",
  "version": "1.2.0",
  "description": "Comprehensive analysis of sophisticated Ethereum smart contract scams targeting airdrop campaigns and cryptocurrency platforms",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "dev": "ts-node --project tsconfig.json src/index.ts",
    "start": "node dist/index.js"
  },
  "keywords": [
    "ethereum",
    "smart-contracts",
    "security",
    "scam-analysis",
    "blockchain",
    "airdrop",
    "fraud-detection"
  ],
  "author": "Security Research Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4eckd/Shuffle.com-Airdrop-Scam.git"
  },
  "bugs": {
    "url": "https://github.com/4eckd/Shuffle.com-Airdrop-Scam/issues"
  },
  "homepage": "https://github.com/4eckd/Shuffle.com-Airdrop-Scam#readme"
}
